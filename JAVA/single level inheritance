public class Main
{
	public static void main(String[] args) {
	 
	 Addsub obj= new Addsub();
	 obj.num1=5;
	 obj.num2=4;
	 obj.sum();
	 System.out.println(obj.result);
	 
	 obj.sub();
	 
	 System.out.println(obj.result);
	}
     
}
class Add
{
    int num1, num2,result=0;
    public void sum()
    {
        result=num1+num2;
    }
}

class Addsub extends Add
{
  
  public void sub()
    {
        result=num1-num2;
    }
   
}


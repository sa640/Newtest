def create_new_dictionary(prices):
    list1=[]
    for i in prices:
        if prices[i]>200:
            result=(i , prices[i])
            list1.append(result)
    
    return dict(list1)
prices = { 'ACME': 45.23,'AAPL': 612.78,'IBM': 205.55,'HPQ': 37.20,'FB': 10.75}
print(create_new_dictionary(prices))

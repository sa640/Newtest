def count_digits_letters(sentence):
    result_list=[]
    c1=0 
    c2=0 
    for i in sentence:
        if i.isalpha():
            c1+=1 
        if i.isdigit():
            c2+=1 
    result_list.append(c1)
    result_list.append(c2)
    return result_list

sentence="Infosys Mysore 570027"
print(count_digits_letters(sentence))
